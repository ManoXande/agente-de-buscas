{
  "name": "agente-de-buscas",
  "version": "1.0.0",
  "description": "Sistema avançado de buscas com integração MCP e LLMs",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "webpack --mode production",
    "setup-mcps": "node scripts/setup-mcps.js",
    "test": "jest",
    "init-db": "node scripts/init-db.js",
    "backup": "node scripts/backup.js create",
    "restore": "node scripts/restore.js interactive",
    "backup-list": "node scripts/backup.js list",
    "lint": "echo \"Linting not configured\"",
    "format": "echo \"Formatting not configured\"",
    "docker:build": "docker build -t agente-buscas .",
    "docker:run": "docker run -p 3000:3000 agente-buscas",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs agente-buscas",
    "pm2:monit": "pm2 monit",
