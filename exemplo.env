# Configurações do Servidor
PORT=3000
NODE_ENV=development

# Configurações da API
API_SECRET=sua_chave_secreta_aqui
JWT_SECRET=sua_jwt_secret_aqui

# Configurações do Claude/Anthropic
ANTHROPIC_API_KEY=sua_chave_anthropic_aqui

# Configurações do OpenAI (opcional)
OPENAI_API_KEY=sua_chave_openai_aqui

# Configurações do GitHub MCP
GITHUB_TOKEN=seu_token_github_aqui

# Configurações do Firecrawl MCP  
FIRECRAWL_API_KEY=sua_chave_firecrawl_aqui

# Configurações do Supabase (opcional)
SUPABASE_URL=sua_url_supabase_aqui
SUPABASE_KEY=sua_chave_supabase_aqui

# Configurações do Banco de Dados
DB_PATH=./data/searches.db

# Configurações de Cache
CACHE_TTL=3600

# Configurações de Rate Limiting
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# Configurações de Logging
LOG_LEVEL=info
LOG_FILE=./logs/app.log

# Configurações dos MCPs
MCP_FIRECRAWL_URL=http://localhost:3001
MCP_GITHUB_URL=http://localhost:3002
MCP_CONTEXT7_URL=http://localhost:3003
MCP_FILESYSTEM_URL=http://localhost:3004

# Configurações de Desenvolvimento
DEBUG=true
PUPPETEER_HEADLESS=true 